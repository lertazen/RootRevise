@model HomeVM
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome </h1>
</div>

<div class="text-center">
    <h2>Open issue: @Model.OpenIssueNumber</h2>
    <h2>High-priority issues: @Model.HighPriorityNumber</h2>
</div>

<div id="issueBarChart" class="d-flex justify-content-center"
   data-open-issues="@Model.OpenIssueNumber"></div>

<div id="test" data-high="@Model.HighPriorityNumber"></div>

@section Scripts {
   <script src="/js/home.js"></script>
}
