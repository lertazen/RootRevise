@model List<ApplicationUser>

<div class="d-flex justify-content-between align-items-center mb-4">
   <div class="pagetitle">
      <h1>User List</h1>
   </div>
</div>


<section class="section">
   <div class="row">
      <div class="col-lg-12">
         <div class="card">
            <div class="card-body mt-3">

               <table id="userTable" style="width: 100%">
                  <thead>
                     <tr>
                        <th>Name</th>
                        <th>User Name</th>
                        <th>Email</th>
                        <th>Role</th>
                        <th></th>
                     </tr>
                  </thead>
               </table>
            </div>
         </div>
      </div>
   </div>
</section>

@section Scripts {
   <script src="/js/user.js"></script>
}
